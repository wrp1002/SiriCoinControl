<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Options</string>
			<key>footerText</key>
			<string>Siri will say this phrase if you hold both volume buttons. If this setting is disabled and you hold both buttons, the result will be random</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enabled</string>
			<key>key</key>
			<string>kEnabled</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Phrase Enabled</string>
			<key>key</key>
			<string>kCustomPhraseEnabled</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Custom Phrase:</string>
			<key>key</key>
			<string>kCustomPhrase</string>
			<key>noAutoCorrect</key>
			<true/>

			<key>default</key>
			<string>Oops, it rolled under the bed</string>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Custom Language Support</string>
			<key>footerText</key>
			<string>Change these if you use a different language for Siri</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Heads Phrase:</string>
			<key>key</key>
			<string>kHeadsPhrase</string>
			<key>noAutoCorrect</key>
			<true/>

			<key>default</key>
			<string>Heads</string>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Tails Phrase:</string>
			<key>key</key>
			<string>kTailsPhrase</string>
			<key>noAutoCorrect</key>
			<true/>

			<key>default</key>
			<string>Tails</string>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Respringing is NOT needed for changes to take effect</string>
		</dict>
		<dict>
			<key>cell</key>
				<string>PSButtonCell</string>
				<key>label</key>
				<string>Reset all settings</string>
				<key>action</key>
				<string>Reset</string>
				<key>isDestructive</key>
				<true/>
				<key>confirmation</key>
				<dict>
					<key>prompt</key>
					<string>Are you sure you want to reset all settings?</string>
					<key>title</key>
					<string>Reset!</string>
					<key>cancelTitle</key>
					<string>Cancel</string>
				</dict>
		</dict>
		<dict>
			<key>cell</key>
				<string>PSButtonCell</string>
				<key>label</key>
				<string>Respring</string>
				<key>action</key>
				<string>Respring</string>
				<key>isDestructive</key>
				<true/>
				<key>confirmation</key>
				<dict>
					<key>prompt</key>
					<string>Are you sure you want to respring your device?</string>
					<key>title</key>
					<string>Respring!</string>
					<key>cancelTitle</key>
					<string>Cancel</string>
				</dict>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Log messages are prefixed with "SiriCoinControl: "</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enable Logging</string>
			<key>key</key>
			<string>kLoggingEnabled</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.wrp1002.siricoincontrol</string>
			<key>PostNotification</key>
			<string>com.wrp1002.siricoincontrol/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Developer</string>
			<key>footerText</key>
			<string>Any small donation is appreciated :)</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Email Me</string>
			<key>action</key>
			<string>OpenEmail</string>
			<key>icon</key>
			<string>email.png</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Reddit</string>
			<key>action</key>
			<string>OpenReddit</string>
			<key>icon</key>
			<string>reddit.png</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Source Code</string>
			<key>action</key>
			<string>OpenGithub</string>
			<key>icon</key>
			<string>github.png</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Support future development</string>
			<key>action</key>
			<string>OpenPaypal</string>
			<key>icon</key>
			<string>paypal.png</string>
		</dict>

	</array>

	<key>title</key>
	<string>SiriCoinControl</string>
</dict>
</plist>
